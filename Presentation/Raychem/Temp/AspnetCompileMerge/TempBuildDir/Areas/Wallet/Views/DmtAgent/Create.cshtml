@model Corno.Raychem.CustomerPortal.Areas.Wallet.Models.UserDMTAgentRT
@{
    Layout = "~/Views/Shared/_LayoutCreate.cshtml";
}

@{
    ViewBag.Index = "Index";
    ViewBag.Create = "Create";
    ViewBag.Details = "Details";
}

@{
    Page.Title = "Agents";
    ViewBag.MainMenu = "Masters";
    ViewBag.CurrentSitemap = "Agents";
    ViewBag.BoxTitle = "Create New DMT Agent";

    ViewBag.Controller = "DmtAgent";

    ViewBag.SubmitAction = ViewBag.Create;
}

@Html.HiddenFor(m => m.Id)

<div class="control-group">
    <label class="control-label">Agent Id :<span style="color:red">*</span>:</label>
    <div class="controls">
        @Html.TextBoxFor(m => m.AgentId, new { id = "AgentId", placeholder = "Enter Agent Id", @class = "input-xlarge", data_rule_required = "true", data_rule_minlength = "2" })
    </div>
</div>
<div class="control-group">
    <label class="control-label">UserName :<span style="color:red">*</span>:</label>
    <div class="controls">
        @Html.TextBoxFor(m => m.UserName, new { id = "UserName", placeholder = "Enter UserName", @class = "input-xlarge", data_rule_required = "true", data_rule_minlength = "2" })
    </div>
</div>
<div class="control-group">
    <label class="control-label">Permanent Address :</label>
    <div class="controls">
        @Html.TextBoxFor(m => m.PermanentAddress, new { id = "PermanentAddress", placeholder = "Enter Permanent Address", @class = "input-xlarge", data_rule_required = "true", data_rule_minlength = "2" })
    </div>
</div>
<div class="control-group">
    <label class="control-label">PinCode :</label>
    <div class="controls">
        @Html.TextBoxFor(m => m.PinCode, new { id = "PinCode", placeholder = "Enter PinCode", @class = "input-xlarge", data_rule_required = "true", data_rule_minlength = "2" })
    </div>
</div>
<hr />

