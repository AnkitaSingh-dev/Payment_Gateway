@model List<Corno.Raychem.CustomerPortal.Areas.Admin.Models.UserViewModel>

@{
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}

@{
    ViewBag.Index = "Index";
    ViewBag.Create = "Register";
    ViewBag.Edit = "Edit";
    ViewBag.Details = "Details";
    ViewBag.Delete = "Delete";
}

@{
    Page.Title = "Users";

    ViewBag.MainHeader = "Users";
    ViewBag.MainMenu = "Transactons";
    ViewBag.Controller = "Account";
    ViewBag.AddNewMethod = "Create";
    ViewBag.CurrentLink = "Users";
}

<table class="table table-bordered dataTable dataTable-scroll-x @*dataTable-grouping*@ dataTable-tools dataTable-colvis">
    <thead>
        <tr>
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone No</th>
            <th>Wallet Balance</th>
            <th>Card Amount</th>
            <th>KYC Status</th>
            <th>Society Member</th>
            <th>Society Id</th>
            <th>CP Id</th>
            <th>Safex Id</th>
            <th>Created On</th>
            <th>Lock/Unlock</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.UserName</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Wallet</td>
            <td>@item.CardAmount</td>
            <td>@item.KYCStatus</td>
            <td>@item.IsSocietyMember</td>
            <td>@item.SocietyId</td>
            <td>@item.MemberId</td>
            <td>@item.CustId</td>
            <td>@item.CreatedOn</td>

        <td class="text-center">
            @if (item.LockoutEnabled)
    {
        <a href='@Url.Action("LockUnlockUser", "Account", new {id = item.Id})'>
            <i class="icon-lock" width="30" height="30"></i>
            @*<img src='data:image/png;base64,' width="30" height="30"/>*@
        </a>
}
else
{
        <a href='@Url.Action("LockUnlockUser", "Account", new {id = item.Id})'>
            <i class="icon-unlock" width="30" height="30"></i>
            @*<img src='data:image/png;base64,' width="30" height="30"/>*@
        </a>
}
        </td>
        <td class="text-center">
            <div class="btn-group">
                <a href="#" data-toggle="dropdown" class="btn btn-danger dropdown-toggle"><i class="icon-cog"></i> <span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li>
                        <a href="@Url.Action("Edit", "Account", new {id = item.Id})">
                            <i class="icon-edit"></i> <span>Edit</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("UserRoles", "Account", new {id = item.Id})">
                            <i class="icon-edit"></i> <span>Roles</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Delete", "Account", new {id = item.Id})">
                            <i class="icon-remove"></i> <span>Delete</span>
                        </a>
                    </li>
                </ul>
            </div>
        </td>
        </tr>
        }
    </tbody>
</table>
